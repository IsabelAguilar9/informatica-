#include <stdio.h>
#include <math.h>
int main()
{
    float f,m, r,c ;
    printf (" Escribir un numero real en grados Faherenheit \n \n");
    scanf ("%f", &f);
    // se hace la parte de abajo, osea la multiplicacion 
    r= f-32;
    m= 5 * r;
    c= m/9;
    printf ( " \n La conversion de los gardos Faherenheit a Celsius es %f \n " , c);
    return 0;
}
