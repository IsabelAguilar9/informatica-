#include <stdio.h>
#include <math.h>
int main()
{
float cp,ia,ap, cuota,x ; 
    printf ( " Por favor escribir los datos correspondintes  \n capital prestado  " );
    scanf("%f", &cp);
    printf ( " interés anual \n " );
    scanf("%f", &ia);
    printf ( " años que dura el préstamo \n " );
    scanf("%f", &ap);
    ap*= 12;
    ia/= 12;
    x= 1 + (ia/100);
   cuota= (cp*ia)/(1-pow(x,- ap));
    printf ( " Sueldo neto: %.2f  \n", cuota );
    return 0;
}
